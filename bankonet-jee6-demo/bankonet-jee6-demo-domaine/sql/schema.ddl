
    create table PUBLIC.ACCOUNTS (
        ACC_TYPE varchar(5) not null,
        ACC_ID bigint generated by default as identity (start with 1),
        ACC_KEY varchar(255) not null,
        ACC_CODE_BANK varchar(5) not null,
        ACC_CODE_CASH_DESK varchar(5) not null,
        ACC_NAME varchar(255) not null,
        ACC_NUMBER bigint not null,
        ACC_AMOUNT_MAX double,
        ACC_LOAN_BEGIN_DATE date,
        ACC_LOAN_END_DATE date,
        ACC_LOAN_AMOUNT double,
        ACC_LOAN_RATE float,
        ACC_SAVINGS_AMOUNT_MAX double,
        ACC_SAVINGS_RATE float,
        ACC_CUS_ID bigint not null,
        ACC_AST_ID integer not null,
        ACC_AMT_ID integer not null,
        ACC_CPT_DEB_ID bigint,
        ACC_CPT_DST_ID bigint,
        primary key (ACC_ID),
        unique (ACC_NAME)
    );

    create table PUBLIC.ACCOUNT_MANAG_TYPE (
        AMT_ID integer generated by default as identity (start with 1),
        AMT_CODE varchar(255),
        AMT_NAME varchar(255),
        primary key (AMT_ID),
        unique (AMT_NAME),
        unique (AMT_CODE)
    );

    create table PUBLIC.ACCOUNT_STATE (
        AST_ID integer generated by default as identity (start with 1),
        AST_CODE varchar(255),
