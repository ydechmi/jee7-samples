    create table PUBLIC.USERS (
        USE_ID bigint generated by default as identity (start with 1),
        USE_ALERT_MODIF bit,
        USE_EMAIL varchar(250),
        USE_LAST_CONNEXION date,
        USE_FIRST_NAME varchar(100) not null,
        USE_LAST_NAME varchar(100) not null,
        USE_LOGIN varchar(50) not null,
        USE_PASSWORD varchar(50) not null,
        USE_CUS_ID bigint,
        USE_LGE_ID integer,
        primary key (USE_ID),
        unique (USE_LOGIN),
        unique (USE_PASSWORD),
        unique (USE_EMAIL)
    );

    create table PUBLIC.USER_ROLE_ASS (
        URA_USE_ID bigint not null,
