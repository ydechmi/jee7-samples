    create table PUBLIC.OPERATIONS (
        OPE_ID bigint generated by default as identity (start with 1),
        OPE_DATE date not null,
        OPE_AMOUNT double not null,
        OPE_CC_ID bigint not null,
        OPE_CD_ID bigint not null,
        OPE_OPT_ID integer not null,
        primary key (OPE_ID)
    );

    create table PUBLIC.OPERATION_TYPE (
        OPT_ID integer generated by default as identity (start with 1),
        OPT_CODE varchar(255),
        OPT_NAME varchar(255),
