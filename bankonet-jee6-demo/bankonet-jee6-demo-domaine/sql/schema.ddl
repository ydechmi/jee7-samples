        primary key (AST_ID),
        unique (AST_CODE),
        unique (AST_NAME)
    );

    create table PUBLIC.ACCOUNT_TYPE (
        ACT_ID integer generated by default as identity (start with 1),
        ACT_CODE varchar(255),
        ACT_NAME varchar(255),
        primary key (ACT_ID),
        unique (ACT_CODE),
        unique (ACT_NAME)
    );

    create table PUBLIC.CUSTOMERS (
        CUS_ID bigint generated by default as identity (start with 1),
        CUS_ADDRESS varchar(250),
        CUS_POSTAL_CODE varchar(50),
        CUS_COUNTRY varchar(50),
        CUS_PHONE varchar(50),
        CUS_TOWN varchar(50),
        primary key (CUS_ID)
    );

    create table PUBLIC.CUSTOMER_ACCOUNT_ASS (
        CUS_ACC_ID bigint generated by default as identity (start with 1),
        CAA_PRINT_DATE date not null,
        CAA_CUS_ID bigint not null,
