<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="       http://java.sun.com/xml/ns/javaee       http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <navigation-rule>
  <from-view-id>/faces/security/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/faces/accounts/home.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <locale-config>
   <default-locale>en</default-locale>
  </locale-config>
  <resource-bundle>
   <base-name>com.sqli.bankonet.web.resources.messages</base-name>
   <var>messages</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>com.sqli.bankonet.web.resources.labels</base-name>
   <var>labels</var>
  </resource-bundle>
 </application>
</faces-config>
